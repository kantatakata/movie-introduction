      <h3>会員一覧</h3>
      <table class='table'>
        <thead>
          <tr>
            <th>image</th>
            <th>名前</th>
            <th>自己紹介文</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= image_tag user.get_profile_image, size:'100x100' %></td>
            <td><%= link_to user.name,user %></td>
            <td><%= user.introduction %></td>
            <td><%= link_to "おすすめ作品",user_path(user.id) %></td>
          </tr>
        <% end %>
        </tbody>
      </table>
